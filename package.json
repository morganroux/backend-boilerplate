{
  "name": "backend",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "prepare": "husky install",
    "predev": "npm run db:generate",
    "prebuild": "npm run db:generate",
    "clean": "rimraf node_modules build",
    "dev": "ts-node-dev --respawn --transpile-only ./src/index.ts",
    "start": "npm run dev",
    "start:prod": "npm run build && node ./build/index.js",
    "build": "tsc",
    "check-unused-exports": "ts-prune -e",
    "check-types": "tsc --noEmit",
    "codegen": "graphql-codegen --config codegen.yml",
    "lint": "eslint ./src --fix --ext .js,.jsx,.ts,.tsx",
    "prettier:write": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prettier:check": "prettier --check \"**/*.{ts,tsx,md}\"",
    "db:migrate:deploy": "npx prisma migrate deploy",
    "db:migrate:dev": "npx prisma migrate dev",
    "db:push": "npx prisma db push",
    "db:seed": "npx prisma db seed",
    "db:format": "npx prisma format",
    "db:generate": "npx prisma generate",
    "db:studio": "npx prisma studio"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^4.2.1",
    "apollo-server-core": "^3.10.1",
    "apollo-server-express": "^3.10.1",
    "axios": "^1.1.2",
    "body-parser": "^1.20.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "database": "*",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "graphql": "^16.6.0",
    "helmet": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "morgan": "^1.10.0",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@graphql-codegen/cli": "^2.11.6",
    "@graphql-codegen/typescript": "^2.7.3",
    "@graphql-codegen/typescript-resolvers": "^2.7.3",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/lodash": "^4.14.184",
    "@types/md5": "^2.3.2",
    "@types/morgan": "^1.9.3",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "eslint": "^8.26.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "prettier": "latest",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  },
  "packageManager": "npm@8.5.1",
  "prisma": {
    "schema": "prisma/schema.prisma",
    "seed": "ts-node prisma/scripts/seed.ts"
  }
}
